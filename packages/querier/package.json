{
  "name": "@owliehq/querier",
  "version": "1.5.0",
  "description": "Http Errors handler",
  "homepage": "https://github.com/owliehq/neatsio/tree/master/packages/querier",
  "keywords": [],
  "main": "dist/querier.js",
  "module": "dist/querier.js",
  "typings": "dist/types/querier.d.ts",
  "files": [
    "dist"
  ],
  "author": "HEN Xavier",
  "bugs": {
    "url": "https://github.com/owliehq/neatsio/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owliehq/neatsio.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts src",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage --runInBand",
    "test:nocoverage": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:prod": "npm run lint && npm run test:nocoverage -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@owliehq/neatsio": "^1.7.4",
    "@semantic-release/git": "^7.0.18",
    "@types/express": "^4.17.2",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.13",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "colors": "^1.4.0",
    "coveralls": "^3.0.9",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "lint-staged": "^8.2.1",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.19.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.7.1",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.2",
    "semantic-release": "^15.13.31",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "query-string": "^6.10.1"
  }
}
