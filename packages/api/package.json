{
  "name": "@owliehq/api",
  "version": "1.13.2",
  "description": "API builder over Express",
  "homepage": "https://github.com/owliehq/neatsio/tree/master/packages/api",
  "keywords": [],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/types/src/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "HEN Xavier",
  "bugs": {
    "url": "https://github.com/owliehq/neatsio/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owliehq/neatsio.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts src",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage --runInBand",
    "test:nocoverage": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:prod": "npm run lint && npm run test:nocoverage -- --no-cache",
    "test:local": "jest --coverage --runInBand",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "debug": "npx ts-node-dev --respawn test/mocks/debug.ts"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-typescript": "^2.1.0",
    "@semantic-release/git": "^7.0.18",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.3",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^23.3.14",
    "@types/mime-types": "^2.1.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^10.17.13",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/pluralize": "0.0.29",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "coveralls": "^3.0.9",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "jest-config": "^25.1.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^8.2.1",
    "lodash.camelcase": "^4.3.0",
    "passport-jwt": "^4.0.0",
    "prettier": "^1.19.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.7.1",
    "rollup": "^1.32.0",
    "semantic-release": "^15.13.31",
    "sequelize-typescript": "^1.1.0",
    "shelljs": "^0.8.3",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0",
    "tslib": "^1.11.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "passport": "^0.4.1"
  },
  "dependencies": {
    "@owliehq/async-wrapper": "^1.1.0",
    "@owliehq/http-errors": "^1.4.0",
    "@owliehq/neatsio": "^1.20.1",
    "cors": "^2.8.5",
    "dot-prop": "^5.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "fast-glob": "^3.2.4",
    "fs-extra": "^8.1.0",
    "passport": "^0.4.1",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "role-acl": "^4.5.4",
    "sequelize": "^5.22.3",
    "type-fest": "^0.20.2"
  }
}
