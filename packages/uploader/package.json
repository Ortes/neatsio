{
  "name": "@owliehq/uploader",
  "version": "1.4.0",
  "description": "Uploader middleware for express",
  "homepage": "https://github.com/owliehq/neatsio/tree/master/packages/uploader",
  "keywords": [],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "HEN Xavier",
  "bugs": {
    "url": "https://github.com/owliehq/neatsio/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owliehq/neatsio.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts src",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage --runInBand",
    "test:nocoverage": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:prod": "npm run lint && npm run test:nocoverage -- --no-cache",
    "test:local": "jest --coverage --runInBand",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@owliehq/http-errors": "^1.2.1",
    "@owliehq/neatsio": "^1.13.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-typescript": "^2.1.0",
    "@semantic-release/git": "^7.0.18",
    "@types/busboy": "^0.2.3",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^23.3.14",
    "@types/md5": "^2.1.33",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^10.17.13",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "colors": "^1.4.0",
    "coveralls": "^3.0.9",
    "cross-env": "^5.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "firebase-admin": "^8.9.2",
    "jest": "^25.1.0",
    "jest-config": "^25.1.0",
    "lint-staged": "^8.2.1",
    "lodash.camelcase": "^4.3.0",
    "md5": "^2.2.1",
    "prettier": "^1.19.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.7.1",
    "rollup": "^1.32.0",
    "semantic-release": "^15.13.31",
    "sequelize": "^5.21.5",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^7.0.1",
    "tslib": "^1.11.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@owliehq/async-wrapper": "^1.1.0",
    "busboy": "^0.3.1",
    "fs-extra": "^8.1.0",
    "mime-types": "^2.1.26"
  }
}
